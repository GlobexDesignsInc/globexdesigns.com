	"license": "MIT",
	"scripts": {
		"build": "next build",
		"deploy": "yarn build && node scripts/deploy.js",
		"dev": "node server.js",
		"eslint": "eslint-cumul8 components pages scripts next.config.js server.js",
		"flow": "flow",
