		"greenlock-express": "2.1.6",
		"next": "6.0.3",
		"react": "16.4.1",
		"react-dom": "16.4.1",
		"react-progressive-image": "0.3.0",
		"react-scroll": "1.7.9",
		"react-tippy": "1.2.2"
	},
	"devDependencies": {
		"@globexdesigns/deploy": "0.2.5",
		"babel-plugin-transform-flow-strip-types": "6.22.0",
		"eslint-config-cumul8": "8.0.0",
		"flow-bin": "0.75.0",
		"flow-typed": "2.4.0",
		"stylelint-config-cumul8": "2.1.0"
